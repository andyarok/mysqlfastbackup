<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
		.selectContent{
			text-align: center;
			padding-top: 10%;
		}
	</ui:style>
	<m:MaterialPanel>
		<m:MaterialHeader>
			<!-- <m:MaterialNavBar>
				<m:MaterialNavBrand title="MySql Fast Backup and Restore">
					
				</m:MaterialNavBrand>
			</m:MaterialNavBar> -->
			<m:MaterialLabel text="What would you like to do?"></m:MaterialLabel>
		</m:MaterialHeader>
		<m:MaterialContainer addStyleNames="{style.selectContent}">
			<m:MaterialRow>
				<m:MaterialColumn grid="l4 m4" offset="l4 m4" >
					<m:MaterialCard backgroundColor="BLUE" ui:field="backupCard" >
						<m:MaterialCardContent>
							<m:MaterialIcon ui:field="backupIcon" iconType="BACKUP" iconSize="LARGE" waves="LIGHT"></m:MaterialIcon>
						</m:MaterialCardContent>
						<m:MaterialCardAction>
							<m:MaterialButton text="Backup"></m:MaterialButton>
						</m:MaterialCardAction>
					</m:MaterialCard>
				</m:MaterialColumn>
			</m:MaterialRow>
			<m:MaterialRow>
				<m:MaterialColumn grid="l4 m4" offset="l4 m4">
					<m:MaterialCard backgroundColor="BLUE" ui:field="restoreCard">
						<m:MaterialCardContent>
							<m:MaterialIcon ui:field="restoreIcon" iconType="RESTORE" iconSize="LARGE" waves="LIGHT"></m:MaterialIcon>
						</m:MaterialCardContent>
						<m:MaterialCardAction>
							<m:MaterialButton text="Restore"></m:MaterialButton>
						</m:MaterialCardAction>
					</m:MaterialCard>
				</m:MaterialColumn>
			</m:MaterialRow>
		</m:MaterialContainer>
	</m:MaterialPanel>
</ui:UiBinder> 